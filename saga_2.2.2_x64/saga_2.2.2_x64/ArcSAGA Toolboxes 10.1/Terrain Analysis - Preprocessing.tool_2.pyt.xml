  <tool name="tool_2" displayname="Sink Removal">
    <parameters>
      <param name="DEM" displayname="DEM" type="Required" direction="Input">
        <dialogReference>&lt;p&gt;DEM&lt;/p&gt;&lt;p&gt;Grid (input)&lt;/p&gt;&lt;p&gt;Digital Elevation Model that has to be processed&lt;/p&gt;</dialogReference>
      </param>
      <param name="SINKROUTE" displayname="Sink Route" type="Optional" direction="Input">
        <dialogReference>&lt;p&gt;Sink Route&lt;/p&gt;&lt;p&gt;Grid (optional input)&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="DEM_PREPROC" displayname="Preprocessed DEM" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Preprocessed DEM&lt;/p&gt;&lt;p&gt;Grid (optional output)&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;&lt;p&gt;Preprocessed DEM. If this is not set changes will be stored in the original DEM grid.&lt;/p&gt;</dialogReference>
      </param>
      <param name="METHOD" displayname="Method" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Method&lt;/p&gt;&lt;p&gt;Choice&lt;/p&gt;&lt;p&gt;Available Choices:&lt;/p&gt;&lt;p&gt;[0] Deepen Drainage Routes&lt;/p&gt;&lt;p&gt;[1] Fill Sinks&lt;/p&gt;&lt;p&gt;Default: 1&lt;/p&gt;</dialogReference>
      </param>
      <param name="THRESHOLD" displayname="Threshold" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Threshold&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 0&lt;/p&gt;</dialogReference>
      </param>
      <param name="THRSHEIGHT" displayname="Threshold Height" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Threshold Height&lt;/p&gt;&lt;p&gt;Floating point&lt;/p&gt;&lt;p&gt;Default: 100.000000&lt;/p&gt;&lt;p&gt;The parameter describes the maximum depth of a sink to be considered for removal [map units]. This allows to exclude deeper sinks from filling.&lt;/p&gt;</dialogReference>
      </param>
    </parameters>
  </tool>
