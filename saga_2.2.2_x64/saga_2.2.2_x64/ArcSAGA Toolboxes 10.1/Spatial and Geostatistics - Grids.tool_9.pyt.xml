  <tool name="tool_9" displayname="Multi-Band Variation">
    <parameters>
      <param name="BANDS" displayname="Grids" type="Required" direction="Input">
        <dialogReference>&lt;p&gt;Grids&lt;/p&gt;&lt;p&gt;Grid list (input)&lt;/p&gt;</dialogReference>
      </param>
