      <param name="YEAR_B" displayname="Year" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Year&lt;/p&gt;&lt;p&gt;Integer&lt;/p&gt;&lt;p&gt;Default: 2015&lt;/p&gt;</dialogReference>
