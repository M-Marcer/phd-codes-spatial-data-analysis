        <dialogReference>&lt;p&gt;Diffuse Insolation&lt;/p&gt;&lt;p&gt;Grid (output)&lt;/p&gt;</dialogReference>
      </param>
      <param name="GRD_TOTAL" displayname="Total Insolation" type="Optional" direction="Output">
